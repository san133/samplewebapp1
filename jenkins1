pipeline {
agent {
label 'buildserver'
}
stages {

stage ('Checkout') 
{
steps
    {
    
        checkout scm
        
    }
      }
      
 stage ('dockerimageBuild')
    {
    steps
    {
        sh " sudo cd /var/lib/jenkins/workspace/nagurbabu/app; sudo docker build -t customer-service . " 
    }
}     
       }
       }
 
